<%-include("partials/header.ejs")%>
<style type="text/css">
    @import url('https://fonts.googleapis.com/css2?family=Lato&display=swap');
    *{
        font-family: 'Lato', sans-serif !important;
    }
    body {
        background-color: black;
    }
      .header {
        margin-top: 0px;
        width: 100%;
        background-color: black !important;
        color: white !important;
        height: 100px;
        font-size: 30px;
        padding-top: 10px;
      }

      #footer {
        position: relative;
        right: 0;
        bottom: 0;
        left: 0;
        padding: 1rem;
        font-size: 20px;
        text-align: center;
      }

      #menu {
        position: fixed;
        top: 20px;
        right: 20px;
        color: white;
      }

      .sidenav {
        height: 100%;
        width: 0;
        position: fixed;
        z-index: 1;
        top: 0;
        right: 0;
        background-color: #444341;
        overflow-x: hidden;
        transition: 0.5s;
        padding-top: 60px;
        font-family: 'Lato', sans-serif;
      }

      .sidenav a {
        padding: 8px 8px 8px 32px;
        text-decoration: none;
        font-size: 20px;
        color: #f1f1f1;
        display: block;
        transition: 0.3s;
        font-family: 'Lato', sans-serif;
      }

      .sidenav a:hover {
        color: black;
        font-family: 'Lato', sans-serif;
      }

      .sidenav .closebtn {
        position: absolute;
        top: 0;
        right: 25px;
        font-size: 36px;
        margin-left: 50px;
      }

      #image {
        width: 150px;
        height: 48px;
      }

      @media screen and (max-height: 450px) {
        .sidenav {
          padding-top: 15px;
        }

        .sidenav a {
          font-size: 18px;
        }
      }

      .title {
        text-align: center;
        font-family: 'Lato', sans-serif;
        color: white !important;
        display: flex;
        flex-direction: row;
        justify-content: center;
        align-items: end;
      }

      .container {
        margin-top: -30px;
        font-family: Lato-Regular;
      }

      @media screen and (max-width: 480px) {
        .header {
          font-size: 20px;
          padding-top: 20px;
        }

        .ham {
          padding-left: 5px;
        }

        #image {
          width: 100px;
          height: 32px;
        }

        #footer {
          font-size: 13px;
        }
      }
</style>
<div class="header">
    <div class="title">
        <a href="/"></a>
        &nbsp;F A Q S
    </div>
</div>
<div class="ui cont" style="background: #000000; padding: 5px; color: #000000;">
