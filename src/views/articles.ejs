    document.head.appendChild(linkTag);
    window.onload = ()=>{
        $('#articles').hide()
        $(window).on('load', function () {
            $('#articleLoader').hide()
            $('#articles').show()
        })

        function openModal(ola, j) {
            var i
            for (i = 1; i < 10; i++) {
                if (i != j) {
                    olaa = '.ui.fullscreen.modal.' + i
                    $(olaa).modal('hide')
                }
            }
            $(ola).modal('show')
            $(ola).modal({
                closable: true,
            })
            $('.cont').dimmer('show')
        }

        function closeModal(ola) {
            $(ola).modal('hide')
        }
    }
</script>

